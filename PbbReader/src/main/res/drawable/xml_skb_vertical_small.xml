<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <inset
            android:insetBottom="4dp"
            android:insetTop="4dp" >
            <shape>
                <corners android:radius="1dip" />

                <gradient
                    android:angle="90"
                    android:endColor="#313236"
                    android:startColor="#313236" />

                <stroke
                    android:width="0px"
                    android:color="#313236" />
            </shape>
        </inset>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <inset
                android:insetBottom="5dp"
                android:insetTop="5dp" >
                <shape>
                    <corners android:radius="10dip" />

                    <gradient
                        android:angle="90"
                        android:endColor="#F32162"
                        android:startColor="#6B0421" />

                    <stroke
                        android:width="2px"
                        />
                </shape>
            </inset>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <inset
                android:insetBottom="5dp"
                android:insetTop="5dp" >
                <shape>
                    <corners android:radius="1dip" />

                    <gradient
                        android:angle="90"
                        android:endColor="#1faa46"
                        android:startColor="#1faa46" />

                    <stroke
                        android:width="0px"
                        android:color="#1faa46" />
                </shape>
            </inset>
        </clip>
    </item>

</layer-list>